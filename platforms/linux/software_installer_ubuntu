#!/bin/sh

sudo apt install -y wget cmake autoconf libtool libtool-bin python3 python3-dev python3-pip ruby zlib1g-dev libbz2-dev libreadline-dev libsqlite3-dev libssl-dev ruby-dev cargo

sudo apt install -y tmux autojump astyle

sudo apt install -y golang nodejs npm

. ${MY_ENV_ROOT}/platforms/common/install_extras

sudo apt install -y vim 


